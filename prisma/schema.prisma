model MappingData {
  id         Int      @id @default(autoincrement())
  name       String
  mainTable  String
  filePath   String
  mapping    String   @db.Text
  status     String
  action     String
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt
  isDeleted  Boolean  @default(false)
}

model Jobs {
  id         Int      @id @default(autoincrement())
  mapId      Int
  status     String
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt
  isDeleted  Boolean  @default(false)
}

model Searches {
  id         Int      @id @default(autoincrement())
  name       String
  page       String
  //Account    Accounts @relation(fields: [accountsId], references: [id])
  accountsId Int
  content    String   @db.Text
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt
}

model Roles {
  id             Int      @id @default(autoincrement())
  name           String
  //Account        Accounts @relation(fields: [accountsId], references: [id])
  access         String
  contactModule  String
  mapsModule     String
  accountsModule String
  otherModule    String?
  created_at     DateTime @default(now())
  updated_at     DateTime @default(now()) @updatedAt
  accountsId     Int
}
